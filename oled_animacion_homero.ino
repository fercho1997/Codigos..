  display.clearDisplay();  // limpiar pantalla
  display.setTextColor(WHITE);  // color
  display.setRotation(0);  // tamaño de letra
  display.setTextWrap(false);  
  display.dim(0); // Establecer brillo (0 es máximo y 1 es un poco tenue)



}  






